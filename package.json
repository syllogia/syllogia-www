{
  "name": "@syllogia/syllogia-www",
  "license": "MIT",
  "scripts": {
    "build": "GENERATE_SOURCEMAP=false SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "sort:package": "npx sort-package-json",
    "start": "BROWSER=none SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "style": "stylelint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "test": "react-scripts test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@bamdadsabbagh/eslint-config": "^1.3.13",
    "@bamdadsabbagh/stylelint-config": "^1.2.25",
    "eslint-plugin-prettier": "^3.1.4",
    "react-scripts": "3.4.1"
  },
  "version": "1.1.31"
}
